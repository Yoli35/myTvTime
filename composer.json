{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "api-platform/core": "^3.1",
    "deeplcom/deepl-php": "^1.5",
    "doctrine/annotations": "1.*",
    "doctrine/doctrine-bundle": "2.*",
    "doctrine/doctrine-migrations-bundle": "3.*",
    "doctrine/orm": "2.*",
    "easycorp/easyadmin-bundle": "^4.4",
    "google/apiclient": "^2.0",
    "jenssegers/agent": "^2.6",
    "knplabs/knp-time-bundle": "^1.20",
    "liip/imagine-bundle": "^2.8",
    "nelmio/cors-bundle": "^2.2",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.16",
    "ramsey/uuid": "4.*",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "6.3.*",
    "symfony/console": "6.3.*",
    "symfony/dependency-injection": "6.3.*",
    "symfony/doctrine-messenger": "6.3.*",
    "symfony/dotenv": "6.3.*",
    "symfony/event-dispatcher": "^6.0",
    "symfony/expression-language": "6.3.*",
    "symfony/flex": "^2",
    "symfony/form": "6.3.*",
    "symfony/framework-bundle": "6.3.*",
    "symfony/google-mailer": "6.3.*",
    "symfony/html-sanitizer": "6.3.*",
    "symfony/http-client": "6.3.*",
    "symfony/intl": "6.3.*",
    "symfony/mailer": "6.3.*",
    "symfony/messenger": "6.3.*",
    "symfony/mime": "6.3.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "6.3.*",
    "symfony/process": "6.3.*",
    "symfony/property-access": "6.3.*",
    "symfony/property-info": "6.3.*",
    "symfony/proxy-manager-bridge": "6.3.*",
    "symfony/runtime": "6.3.*",
    "symfony/security-bundle": "6.3.*",
    "symfony/serializer": "6.3.*",
    "symfony/stimulus-bundle": "^2.10",
    "symfony/string": "6.3.*",
    "symfony/translation": "6.3.*",
    "symfony/twig-bundle": "6.3.*",
    "symfony/ux-dropzone": "2.8.*",
    "symfony/ux-live-component": "*",
    "symfony/ux-twig-component": "^2.4",
    "symfony/ux-typed": "^2.6",
    "symfony/validator": "6.3.*",
    "symfony/web-link": "6.3.*",
    "symfony/webapp-meta": "^1.0",
    "symfony/webpack-encore-bundle": "^1.16",
    "symfony/yaml": "6.3.*",
    "symfonycasts/verify-email-bundle": "^1.10",
    "twig/cssinliner-extra": "3.*",
    "twig/extra-bundle": "3.*",
    "twig/intl-extra": "3.*",
    "twig/twig": "3.*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "php-http/discovery": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.3.*"
    }
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "icanhazstring/composer-unused": "^0.8.4",
    "symfony/debug-bundle": "6.3.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/stopwatch": "6.3.*",
    "symfony/web-profiler-bundle": "6.3.*"
  }
}
